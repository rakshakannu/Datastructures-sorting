// O[n square] - worst case
// works well for small arrays, doesn't require extra space and it is a stable sorting algorithm

void isort(int arr[], int n)
{
  for(int i=1;i<n;i++)
  {
  int key=arr[i];
  int j=i=1;
  while(j>=0 && arr[j]>key)
  {
    arr[j+1]=arr[j];
    j--;
  }
  arr[j+1]=key;
  }
}
